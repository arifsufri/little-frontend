# API Configuration
NEXT_PUBLIC_API_URL="http://localhost:4000"

# Environment
NODE_ENV=development

# Keep-Alive Service Configuration
# Set to 'true' to enable keep-alive service in development (disabled by default)
NEXT_PUBLIC_ENABLE_KEEP_ALIVE=false

# Keep-alive ping interval in minutes (default: 10 minutes)
# Render spins down after 15 minutes of inactivity, so 10 minutes is safe
NEXT_PUBLIC_KEEP_ALIVE_INTERVAL=10
